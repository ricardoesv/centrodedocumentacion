<p id="notice"><%= notice %></p>

<h1>Listing Documents</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Subtitulo</th>
      <th>Autor</th>
      <th>Colaborador</th>
      <th>Clasificacion</th>
      <th>Editorial</th>
      <th>Edicion</th>
      <th>Edicion</th>
      <th>Serie</th>
      <th>Traduccion</th>
      <th>Bibliografia</th>
      <th>Indice</th>
      <th>Apendice</th>
      <th>Apendice</th>
      <th>Isbn</th>
      <th>Contenido</th>
      <th>Cantidad</th>
      <th>Acervo</th>
      <th>Ano publicacion</th>
      <th>Anaquel</th>
      <th>Peldano</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documents.each do |document| %>
      <tr>
        <td><%= document.titulo %></td>
        <td><%= document.subtitulo %></td>
        <td><%= document.autor %></td>
        <td><%= document.colaborador %></td>
        <td><%= document.clasificacion %></td>
        <td><%= document.editorial %></td>
        <td><%= document.edicion %></td>
        <td><%= document.edicion %></td>
        <td><%= document.serie %></td>
        <td><%= document.traduccion %></td>
        <td><%= document.bibliografia %></td>
        <td><%= document.indice %></td>
        <td><%= document.apendice %></td>
        <td><%= document.apendice %></td>
        <td><%= document.isbn %></td>
        <td><%= document.contenido %></td>
        <td><%= document.cantidad %></td>
        <td><%= document.acervo %></td>
        <td><%= document.ano_publicacion %></td>
        <td><%= document.anaquel %></td>
        <td><%= document.peldano %></td>
        <td><%= link_to 'Show', document %></td>
        <td><%= link_to 'Edit', edit_document_path(document) %></td>
        <td><%= link_to 'Destroy', document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

