<p id="notice"><%= notice %></p>

<h1>Listing Audios</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Subtitulo</th>
      <th>Autor</th>
      <th>Colaborador</th>
      <th>Clasificacion</th>
      <th>Editorial</th>
      <th>Edicion</th>
      <th>Edicion</th>
      <th>Serie</th>
      <th>Traduccion</th>
      <th>Bibliografia</th>
      <th>Indice</th>
      <th>Apendice</th>
      <th>Apendice</th>
      <th>Isbn</th>
      <th>Contenido</th>
      <th>Cantidad</th>
      <th>Acervo</th>
      <th>Ano publicacion</th>
      <th>Anaquel</th>
      <th>Peldano</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @audios.each do |audio| %>
      <tr>
        <td><%= audio.titulo %></td>
        <td><%= audio.subtitulo %></td>
        <td><%= audio.autor %></td>
        <td><%= audio.colaborador %></td>
        <td><%= audio.clasificacion %></td>
        <td><%= audio.editorial %></td>
        <td><%= audio.edicion %></td>
        <td><%= audio.edicion %></td>
        <td><%= audio.serie %></td>
        <td><%= audio.traduccion %></td>
        <td><%= audio.bibliografia %></td>
        <td><%= audio.indice %></td>
        <td><%= audio.apendice %></td>
        <td><%= audio.apendice %></td>
        <td><%= audio.isbn %></td>
        <td><%= audio.contenido %></td>
        <td><%= audio.cantidad %></td>
        <td><%= audio.acervo %></td>
        <td><%= audio.ano_publicacion %></td>
        <td><%= audio.anaquel %></td>
        <td><%= audio.peldano %></td>
        <td><%= link_to 'Show', audio %></td>
        <td><%= link_to 'Edit', edit_audio_path(audio) %></td>
        <td><%= link_to 'Destroy', audio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Audio', new_audio_path %>
